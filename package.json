{
  "name": "udid.report",
  "private": true,
  "version": "1.0.0",
  "description": "UDID.Report",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm install && node app.js $(node -e 'console.log(process.env.PORT || 3001)')",
    "postinstall": "npm install cpy-cli -g && cpy --flat ./node_modules/framework7-icons/fonts/*.* ./www/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./www/fonts/ && cpy --flat ./node_modules/framework7/*.js ./www/framework7 && cpy --flat ./node_modules/framework7/*.css ./www/framework7 && cpy --flat ./node_modules/framework7/*.map ./www/framework7"
  },
  "browserslist": [
    "IOS >= 13",
    "Safari >= 13",
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "Samsung >= 12"
  ],
  "dependencies": {
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.6",
    "dom7": "^4.0.4",
    "express": "^4.18.1",
    "framework7": "^7.0.7",
    "framework7-icons": "^5.0.5",
    "helmet": "^6.0.0",
    "material-icons": "^1.11.11",
    "minimist": "^1.2.6",
    "skeleton-elements": "^4.0.0",
    "swig": "^1.4.2",
    "swiper": "^8.3.2",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "cpy-cli": "^4.2.0",
    "http-server": "^14.1.1"
  }
}
